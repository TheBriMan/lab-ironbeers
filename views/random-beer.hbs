<div>
    {{#each randomBeer}}
    <img src="{{this.image_url}}" />
    <h2>{{this.name}}</h2>
    <p>{{this.description}}</p>
    <p>{{this.tagline}}</p>
    <p>Food Pairing</p>
    <ul>
        {{#each food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <p>Brewer Tips: {{this.brewers_tips}}</p>
    {{/each}}
    
</div>